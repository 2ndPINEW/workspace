{
  "name": "TabGroupManager",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "タブグループ管理",
  "background": {
    "service_worker": "index.js"
  },
  "permissions": [
    "tabGroups",
    "tabs",
    "notifications"
  ],
  "commands": {
    "createNewTabeInActiveTabGroup": {
      "suggested_key": {
        "default": "Ctrl+K",
        "mac": "Command+K"
      },
      "description": "タブグループ内に新規タブを開きます"
    }
  }
}