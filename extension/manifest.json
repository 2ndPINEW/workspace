{
  "name": "TabSessionManager",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "タブセッション管理",
  "background": {
    "service_worker": "index.js"
  },
  "permissions": [
    "tabGroups",
    "tabs",
    "notifications",
    "unlimitedStorage",
    "storage"
  ],
  "commands": {
    "switchMode": {
      "suggested_key": {
        "default": "Ctrl+K",
        "mac": "Command+K"
      },
      "description": "モード切り替える"
    },
    "tabMove": {
      "suggested_key": {
        "default": "Ctrl+U",
        "mac": "Command+U"
      },
      "description": "タブを移動する"
    }
  }
}